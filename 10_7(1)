#include <stdio.h>
#define _CRT_SECURE_NO_WARNINGS

main() {
	int x, y;

	scanf("%d", &x);
	scanf("%d", &y);
	if (((x > 0) && (x <= 1000)) && ((y > 0) && (y <= 1000))) {printf("1");}
	else if (((x < 0) && (x >= -1000)) && ((y > 0) && (y <= 1000))) {printf("2");}
	else if (((x < 0) && (x >= -1000)) && ((y < 0) && (y >= -1000))) { printf("3");}
	else if (((x > 0) && (x <= 1000)) && ((y < 0) && (y >= -1000))) { printf("4");}
	}
  
  //백준 14681
